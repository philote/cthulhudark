{{! Notes Tab }}
<section
	class="tab notes {{tab.cssClass}}"
	data-group="primary"
	data-tab="notes"
>
	<prose-mirror
		name="system.notes"
		compact="true"
		data-document-u-u-i-d="{{actor.uuid}}"
		value="{{system.notes}}"
		collaborate="true"
		toggled="true"
	>
		{{{enrichedNotes}}}
	</prose-mirror>
</section>